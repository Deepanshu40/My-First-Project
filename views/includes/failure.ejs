<% if (failure && failure.length) { %>
    <div class="alert alert-warning alert-dismissible fade show col-8 offset-2 col-md-6 offset-md-3 mt-4" role="alert">
        <%= failure %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
    <% } %>
      